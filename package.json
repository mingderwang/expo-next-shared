{
  "name": "expo-next-shared",
  "version": "0.0.0",
  "description": "An example of a NextJS and Expo codebase sharing code and package.json",
  "author": "Jimmy Lee (@meanJim)",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "dev": "babel-node index.js --presets es2015,stage-1,stage-2,stage-3",
    "build": "NODE_ENV=production next build",
    "heroku-postbuild": "chmod -R +x node_modules/next/dist/bin/next-* && NODE_ENV=production npm run build",
    "start": "NODE_ENV=production babel-node index.js --presets es2015,stage-1,stage-2,stage-3"
  },
  "engines": {
    "node": "^7.2.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmylee/expo-next-shared.git"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "cors": "^2.8.4",
    "expo": "^22.0.0",
    "express": "^4.15.3",
    "glamor": "^2.20.40",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.8.2",
    "next": "^4.1.2",
    "next-routes": "^1.1.0",
    "react": "16.0.0-beta.5",
    "react-dom": "^16.0.0",
    "react-native": "^0.52.1",
    "react-string-replace": "^0.4.1"
  }
}
