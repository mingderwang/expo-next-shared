{
  "expo": {
    "name": "expo-next-shared",
    "description": "An example of a NextJS and Expo codebase sharing code and package.json. https://github.com/jimmylee/expo-next-shared",
    "slug": "expo-next-shared",
    "privacy": "public",
    "sdkVersion": "22.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "assetExts": ["ttf", "mp4"]
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
